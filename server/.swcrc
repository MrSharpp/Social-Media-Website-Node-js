{
    "$schema": "https://json.schemastore.org/swcrc",
    "sourceMaps": true,
    "module": {
      "type": "commonjs"
    },
    "jsc": {
        "paths": {
            "~/*": ["./*"],
            "src": ["./src/*"]
        
          },
      "target": "es2020",
      "parser": {
        "syntax": "typescript",
        "decorators": true,
        "dynamicImport": true
      },
      "transform": {
        "legacyDecorator": true,
        "decoratorMetadata": true
      },
      "keepClassNames": true,
      "baseUrl": "./../"
    },
    "minify": false
  }
  