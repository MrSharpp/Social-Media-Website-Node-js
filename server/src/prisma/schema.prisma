datasource db {
    url      = "file:./database.db"
    provider = "sqlite"
}

generator client {
    provider = "prisma-client-js"
}

model Collections {
    id      Int      @id @default(autoincrement())
    columns String
    Records Record[]
}

model Record {
    id            Int         @id @default(autoincrement())
    collection    Collections @relation(fields: [collectionsId], references: [id])
    collectionsId Int
}
